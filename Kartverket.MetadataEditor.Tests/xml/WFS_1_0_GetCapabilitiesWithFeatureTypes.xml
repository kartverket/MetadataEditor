  <Service>
    <Name>MapServer WFS</Name>
    <Title>Data fra Artskart</Title>
    <Abstract>Artsdatabanken publiserer via denne tjenesten data fra tjenesten Artskart</Abstract>
    <Keywords>
      Funn av arter
      species observations
    </Keywords>
    <OnlineResource>http://artskart.artsdatabanken.no</OnlineResource>
    <Fees>free</Fees>
    <AccessConstraints>read online</AccessConstraints>
  </Service>

  <Capability>
    <Request>
      <GetCapabilities>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://kart.artsdatabanken.no/WMS/artskart.aspx?" />
          </HTTP>
        </DCPType>
        <DCPType>
          <HTTP>
            <Post onlineResource="http://kart.artsdatabanken.no/WMS/artskart.aspx?" />
          </HTTP>
        </DCPType>
      </GetCapabilities>
      <DescribeFeatureType>
        <SchemaDescriptionLanguage>
          <XMLSCHEMA/>
        </SchemaDescriptionLanguage>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://kart.artsdatabanken.no/WMS/artskart.aspx?" />
          </HTTP>
        </DCPType>
        <DCPType>
          <HTTP>
            <Post onlineResource="http://kart.artsdatabanken.no/WMS/artskart.aspx?" />
          </HTTP>
        </DCPType>
      </DescribeFeatureType>
      <GetFeature>
        <ResultFormat>
          <GML2/>
        </ResultFormat>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://kart.artsdatabanken.no/WMS/artskart.aspx?" />
          </HTTP>
        </DCPType>
        <DCPType>
          <HTTP>
            <Post onlineResource="http://kart.artsdatabanken.no/WMS/artskart.aspx?" />
          </HTTP>
        </DCPType>
      </GetFeature>
    </Request>
  </Capability>

