@model Kartverket.MetadataEditor.Models.MetadataViewModel

<div class="form-group">
    <label class="col-sm-2 control-label">Bruksbegrensninger</label>
    <div class="col-sm-9">
        @Html.EditorFor(m => m.UseLimitations)
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label">Tilgangsrestriksjoner</label>
    <div class="col-sm-9">
        @Html.DropDownList("AccessConstraints", ViewBag.AccessConstraintValues as SelectList, String.Empty, new { @class = "form-control" })
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label">Brukerrestriksjoner</label>
    <div class="col-sm-9">
        @Html.DropDownList("UseConstraints", ViewBag.UseConstraintValues as SelectList, String.Empty, new { @class = "form-control" })
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label">Andre restriksjoner</label>
    <div class="col-sm-9">
        @Html.EditorFor(m => m.OtherConstraints)
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label">Sikkerhetsnivå</label>
    <div class="col-sm-9">
        @Html.DropDownList("SecurityConstraints", ViewBag.SecurityConstraintValues as SelectList, String.Empty, new { @class = "form-control" })
    </div>
</div>
