@using Newtonsoft.Json;
@using Newtonsoft.Json.Linq;
@{
    ViewBag.Title = "Mine registerdata";
}

<div id="registerdata">
    <h1>Mine registerdata</h1>
    @{WebClient c = new WebClient();
    c.Encoding = System.Text.Encoding.UTF8;
    var data = c.DownloadString(ViewBag.RegisterOrganizationUrl);
    var o = JArray.Parse(data);
    foreach (var registerData in o)
    {
        <div class="row">
            <div class="col-sm-4"><a href="@registerData["showUrl"]" target="_blank">@registerData["name"]</a></div>
            <div class="col-sm-4">@registerData["description"]</div>
            <div class="col-sm-4">@registerData["status"]</div>
        </div>
    <hr />
    }
}

