@model Kartverket.MetadataEditor.Models.MetadataViewModel
<div class="form-group">
    <label class="col-sm-2 control-label">Oppdateringshyppighet</label>
    <div class="col-sm-9">
        @Html.EditorFor(m => m.MaintenanceFrequency)
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label">Målestokkstall</label>
    <div class="col-sm-9">
        @Html.EditorFor(m => m.ResolutionScale)
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label">Standard (produktspesifikasjon)</label>
    <div class="col-sm-9">
        @Html.EditorFor(m => m.QualitySpecificationTitle)
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label">Dato</label>
    <div class="col-sm-2">
        @Html.EditorFor(m => m.QualitySpecificationDate)
    </div>
    <label class="col-sm-2 control-label">Type dato</label>
    <div class="col-sm-2">
        @Html.EditorFor(m => m.QualitySpecificationDateType)
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label">Forklaring av resultat</label>
    <div class="col-sm-9">
        @Html.EditorFor(m => m.QualitySpecificationExplanation)
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label">Godkjent</label>
    <div class="col-sm-9">
        @Html.EditorFor(m => m.QualitySpecificationResult)
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label">Prosesshistorie</label>
    <div class="col-sm-9">
        @Html.EditorFor(m => m.ProcessHistory)
    </div>
</div>
